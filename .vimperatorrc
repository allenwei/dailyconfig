" settings
"set activate=quickmark,paste
"set nobeep
"set complete=sbhf
"set defsearch=google
set guioptions=mT
set maxhints=1000
set complete=sbhfS
set defsearch=google
set guioptions=T
set hlsearch 
set incsearch
set smartcase

" may affect performance
"set preload 

set showmode
set showstatuslinks

"set wildmode=list:longest

" key mappings
map <A-n> :tabnew<CR>
map <A-.> :tabnext<CR>
map <A-,> :tabprev<CR>
map <A-q> :tabclose<CR>

map q :q<CR>


echo .vimperatorrc sourced!

" vim: ft=vimperator sw=2 sts=2
